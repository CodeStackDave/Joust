{
	"extends": [
		"tslint:latest",
		"tslint-react"
	],
	"rules": {
		"indent": [
			true,
			"tabs"
		],
		"one-line": [
			true,
			"check-open-brace",
			"check-whitespace"
		],
		"interface-name": false,
		"switch-default": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"object-literal-shorthand": false
	}
}
